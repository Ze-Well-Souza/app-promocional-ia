# Ambiente de Staging
VITE_APP_ENV=staging
VITE_APP_NAME="App Promocional IA - STAGING"
VITE_APP_VERSION=1.0.0-staging
VITE_APP_DEBUG=true
VITE_APP_API_TIMEOUT=30000
VITE_APP_ENABLE_ANALYTICS=true
VITE_APP_ENABLE_ERROR_REPORTING=true

# URLs de API (staging)
VITE_APP_API_BASE_URL=https://staging-api.app-promocional-ia.com/api

# Configurações de staging
VITE_APP_MOCK_API=false
VITE_APP_ENABLE_DEVTOOLS=true
VITE_APP_LOG_LEVEL=info

# Rate limiting mais restritivo para testes
VITE_APP_RATE_LIMIT_REQUESTS=50
VITE_APP_RATE_LIMIT_WINDOW=60000

# Configurações de teste
VITE_APP_TEST_MODE=true
VITE_APP_ENABLE_TEST_DATA=true

# Chaves de API de staging (configurar via CI/CD)
# VITE_APP_OPENAI_API_KEY=
# VITE_APP_CLAUDE_API_KEY=
# VITE_APP_GEMINI_API_KEY=